  },
  "author": "Bradley DeJong",
  "license": "ISC",
  "dependencies": {
    "bel": "^5.1.7",
    "folktale": "^2.1.0",
    "nanocomponent": "^6.5.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "budo": "^11.2.0",
    "choo": "^6.10.1",
    "sheetify": "^7.3.0",
